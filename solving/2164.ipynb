    "# timeout\n",
    "num = int(input())\n",
    "\n",
    "num_list = list(range(1,num+1))\n",
    "\n",
    "while len(num_list) != 1:\n",
    "    del num_list[0]\n",
    "    num_list += [num_list.pop(0)]\n",
    "\n",
    "print(num_list[0])"
   ]
