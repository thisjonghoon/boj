    "# memory : 117844 KB\n",
    "# time : 172 ms\n",
    "\n",
    "import sys\n",
    "from collections import deque\n",
    "\n",
    "k = int(sys.stdin.readline())\n",
    "stack = deque()\n",
    "\n",
    "for _ in range(k):\n",
    "    n = int(sys.stdin.readline())\n",
    "    if n == 0:\n",
    "        stack.pop()\n",
    "    else:\n",
    "        stack.append(n)\n",
    "\n",
    "print(sum(stack))"
   ]
