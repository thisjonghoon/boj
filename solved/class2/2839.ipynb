    "# memory : 114328 KB\n",
    "# time : 116 ms\n",
    "\n",
    "import sys\n",
    "\n",
    "n = int(sys.stdin.readline())\n",
    "\n",
    "ans = n\n",
    "\n",
    "for x in range(n//3):\n",
    "    for y in range(n//5):\n",
    "        if (3*x+5*y)==n:\n",
    "            ans = min(ans,x+y)\n",
    "\n",
    "if ans != n:\n",
    "    print(ans)\n",
    "else:\n",
    "    print(-1)"
